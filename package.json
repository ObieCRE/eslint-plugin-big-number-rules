{
  "name": "@obierisk/eslint-plugin-big-number-rules",
  "version": "1.9.3",
  "description": "Enforce finance-safe calculations using BigNumber instead of native JavaScript arithmetic and Math functions.",
  "author": "obierisk",
  "license": "MIT",
  "keywords": [
    "big number",
    "eslint plugin",
    "ieee754",
    "binary floating point",
    "finance safe",
    "currency safe"
  ],
  "homepage": "https://github.com/ObieCRE/eslint-plugin-big-number-rules",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ObieCRE/eslint-plugin-big-number-rules.git"
  },
  "bugs": {
    "url": "https://github.com/ObieCRE/eslint-plugin-big-number-rules/issues",
    "email": "engineering@obierisk.om"
  },
  "engines": {
    "node": ">=10"
  },
  "main": "lib/index.js",
  "files": [
    "lib/**/*.js",
    "CHANGELOG.md",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "perf": "node ./tests/perf.js",
    "test": "node ./tests/src/all.js",
    "lint": "pnpx eslint lib/*.js tests/*.js",
    "test:watch": "nodemon ./tests/src/all.js",
    "build:all": "echo 'No build process'"
  },
  "dependencies": {
    "match-iz": "^3.7.1",
    "sift-r": "^1.3.2"
  },
  "devDependencies": {
    "eslint": "^8.16.0",
    "glob": "^8.0.3",
    "load-perf": "^0.2.0",
    "nodemon": "^2.0.16"
  },
  "directories": {
    "lib": "lib",
    "test": "tests"
  }
}
